  <body>
    <nav class="navbar navbar-dark bg-dark">
        <a class="navbar-brand">COVID-19</a>
        <form class="form-inline" action="/add" method="GET">
          <button class="btn btn-primary my-2 my-sm-0" type="submit">Add Patient</button>
        </form>
      </nav>
<div class="container-fluid">
  <div class="row">
    <main role="main" class="col-md-9 ml-sm-auto col-lg-10 px-md-4">
      <div class="d-flex justify-content-between flex-wrap flex-md-nowrap align-items-center pt-3 pb-2 mb-3 border-bottom">
        <h1 class="h2">COVID-19 Dashboard</h1>
        <div class="btn-toolbar mb-2 mb-md-0">
            <div class="btn-group mr-2">
                <button type="button" class="btn btn-sm btn-outline-secondary"><%= date %> </button>
              </div>
          <button type="button" class="btn btn-sm btn-outline-secondary dropdown-toggle">
            <span data-feather="calendar"></span>
            Last 30 Days
          </button>
        </div>
      </div>

      <canvas class="my-4 w-100" id="myChart" width="900" height="500"></canvas>
        <!-- Features -->

  <section id="features" class="white">
    <div class="row row1">
      <div class="col-md-3">
        <strong class="fas blue"><%= data[0].Confirmed %> </strong>
        <p class="blue">Total Confirmed Cases</p> 
      </div> 
      <div class="col-md-3">
        <strong class="fas red"><%= data[0].Active %></strong>
        <p class="red">Total Active Cases </p>
      </div>
      <div class="col-md-3">
        <strong class="fas green"><%= data[0].Recovered %></strong>  
        <p class="green">Total Cases Recovered.</p>
      </div>
      <div class="col-md-3">
        <strong class="fas gray"><%= data[0].Death %></strong>  
        <p class="gray"> No. of Deaths.</p>
      </div>
    </div>
  </section>

      <h2>State-wise COVID-19 Cases</h2>
      <div class="table-responsive">
        <table class="table table-striped table-sm">
          <thead>
            <tr>
              <th>State</th>
              <th class="blue">Confirmed</th>
              <th class="red">Active</th>
              <th class="green">Recovered</th>
              <th class="gray">Deceased</th>
            </tr>
          </thead>
          <tbody>
        <%for(var i=1;i<data.length;i++){%>
            <tr>
              <td ><%=data[i].State%> </td>
              <td class="blue"><%=data[i].Confirmed%></td>
              <td class="red"><%=data[i].Active%></td>
              <td class="green"><%=data[i].Recovered%></td>
              <td class="gray"><%=data[i].Death%></td>
            </tr>
        <%}%>
          </tbody>
        </table>
      </div>
    </main>
  </div>
</div>
